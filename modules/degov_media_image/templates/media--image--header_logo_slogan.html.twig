{#
/**
 * @file
 * Default theme implementation to present a media entity.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
 #}
{% set media_classes = [
'image',
'clearfix',
] %}
<figure{{ attributes.addClass(media_classes) }}>
    {{ content.image|field_value }}
</figure>
